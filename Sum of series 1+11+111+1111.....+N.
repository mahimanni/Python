N=int(input("Enter value of N:"))
s=0
for i in range(1,N+1):#series from 1 to Nth term
   a=""#empty string
   for j in range(1,i+1):#loop from 1 to i
      a=a+"1"#adding string "1" in variable a till i(eg. if i=2 then a=11,if i=4 then a=1111)
   s=s+int(a) #converting string a to int so that we can add it in sum because strings cannot be added numerically
print("Sum=",s)
